<?xml version="1.0" encoding="UTF-8"?>
<devices>
    <default version="1.0">
        <manufacturerName> Atmel </manufacturerName>
        <deviceAlgorithm>SPI flash</deviceAlgorithm>
        <maxBitrate>20000</maxBitrate>
        <eraseBeforeWrite>false</eraseBeforeWrite>
        <!-- Buffer 1 to Main Memory Page w/ Built-in Erase -->
        <writeInstruction>0x83</writeInstruction>
        <!-- Buffer 1 Write -->
        <writeBufferInstruction>0x84</writeBufferInstruction>
        <addressWidth>3</addressWidth>
        <readDummyBytes>4</readDummyBytes>
        <readInstruction>0xe8</readInstruction>
        <hasEraseAll>false</hasEraseAll>
        <eraseInstruction>0x50</eraseInstruction>
        <deviceBusyBit>0x80</deviceBusyBit>
        <readStatusRegisterInstruction>0xd7</readStatusRegisterInstruction>
        <writeStatusRegisterInstruction>0</writeStatusRegisterInstruction>
        <busyBitPolarity>true</busyBitPolarity>
        <readDeviceIdInstruction>0x9f</readDeviceIdInstruction>
        <writeEnableBits>0</writeEnableBits>
        <writeEnableInstruction>0</writeEnableInstruction>
        <blockProtectBits>0x2</blockProtectBits>
        <deviceConnectedMask>0x3d</deviceConnectedMask>
    </default>


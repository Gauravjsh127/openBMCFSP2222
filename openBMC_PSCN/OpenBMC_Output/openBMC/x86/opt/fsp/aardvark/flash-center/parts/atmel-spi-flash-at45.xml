    <!-- There are two devices for each 'D' device below.
       - One corresponds to the native page size, which is a multiple of 264,
       - and the other to the power-of-two pagesize which is a multiple
       - of 256.
      -->
    <device version="1.0">
        <deviceName>AT45DB041D/264</deviceName>
        <deviceDescription>
             Atmel AT45DB041D 512 Kilobyte SPI Flash, 264 byte pages
        </deviceDescription>
        <maxBitrate>66000</maxBitrate>
        <capacity>2048 * 264</capacity>
        <writeSize>264</writeSize>
        <writeTime>14000</writeTime>
        <eraseSize>2112</eraseSize>
        <eraseTime>30000</eraseTime>
        <expectedDeviceId>\x1f\x24\x00\x00</expectedDeviceId>
        <deviceConnectedBits>0x1c</deviceConnectedBits>
    </device>

    <device version="1.0">
        <deviceName>AT45DB041D/256</deviceName>
        <deviceDescription>
             Atmel AT45DB041D 512 Kilobyte SPI Flash, 256 byte pages
        </deviceDescription>
        <maxBitrate>66000</maxBitrate>
        <capacity>2048 * 256</capacity>
        <writeSize>256</writeSize>
        <writeTime>14000</writeTime>
        <eraseSize>2048</eraseSize>
        <eraseTime>30000</eraseTime>
        <readDeviceIdInstruction>0x9f</readDeviceIdInstruction>
        <expectedDeviceId>\x1f\x24\x00\x00</expectedDeviceId>
        <!-- deviceConnectedBits includes the page size bit indicating 256
             byte pages -->
        <deviceConnectedBits>0x1d</deviceConnectedBits>
    </device>


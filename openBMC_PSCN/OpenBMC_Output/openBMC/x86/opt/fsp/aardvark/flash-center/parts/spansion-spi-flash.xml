<?xml version="1.0" encoding="UTF-8"?>
<devices>
    <default version="1.0">
        <manufacturerName> Spansion </manufacturerName>
        <deviceAlgorithm>SPI flash</deviceAlgorithm>
        <maxBitrate>50*1000</maxBitrate>
        <addressWidth>3</addressWidth>
        <readDummyBytes>1</readDummyBytes>
        <writeSize>256</writeSize>
        <eraseSize>64*1024</eraseSize>
        <writeTime>1500</writeTime>
        <eraseTime>500*1000</eraseTime>
        <blockProtectBits>0x1c</blockProtectBits>
        <writeStatusRegisterTime>67000</writeStatusRegisterTime>
        <hasEraseAll>true</hasEraseAll>
        <readInstruction>0x0b</readInstruction>
        <readDeviceIdInstruction>0x9f</readDeviceIdInstruction>
        <eraseInstruction>0xd8</eraseInstruction>
        <eraseAllInstruction>0xc7</eraseAllInstruction>
        <deviceConnectedBits>0x00</deviceConnectedBits>
        <deviceConnectedMask>0x60</deviceConnectedMask>
    </default>

    <device version="1.0">
        <deviceName>S25FL004A</deviceName>
        <deviceDescription>
            Spansion S25FL004A 512 Kilobyte SPI Flash
        </deviceDescription>
        <capacity>512*1024</capacity>
        <eraseAllTime>3000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x12</expectedDeviceId>
    </device>

    <device version="1.0">
        <deviceName>S25FL008A</deviceName>
        <deviceDescription>
            Spansion S25FL008A 1 Megabyte SPI Flash
        </deviceDescription>
        <capacity>1024*1024</capacity>
        <eraseAllTime>6000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x13</expectedDeviceId>
    </device>

    <device version="1.0">
        <deviceName>S25FL016A</deviceName>
        <deviceDescription>
            Spansion S25FL016A 2 Megabyte SPI Flash
        </deviceDescription>
        <capacity>2*1024*1024</capacity>
        <writeTime>1400</writeTime>
        <eraseAllTime>10*1000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x14</expectedDeviceId>
    </device>

    <device version="1.0">
        <deviceName>S25FL032A</deviceName>
        <deviceDescription>
            Spansion S25FL032A 4 Megabyte SPI Flash
        </deviceDescription>
        <capacity>4*1024*1024</capacity>
        <eraseAllTime>25*1000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x15</expectedDeviceId>
    </device>

    <device version="1.0">
        <deviceName>S25FL064A</deviceName>
        <deviceDescription>
            Spansion S25FL064A 8 Megabyte SPI Flash
        </deviceDescription>
        <capacity>8*1024*1024</capacity>
        <eraseTime>1500*1000</eraseTime>
        <eraseAllTime>192*1000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x16</expectedDeviceId>
    </device>


    <device version="1.0">
        <deviceName>S25FL040A Uniform</deviceName>
        <deviceDescription>
            Spansion S25FL040A 512 Kilobyte SPI Flash, uniform
        </deviceDescription>
        <capacity>512*1024</capacity>
        <eraseSize>64*1024</eraseSize>
        <eraseAllTime>3000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x12</expectedDeviceId>
    </device>

    <!--
      Uncomment the following XML to use the S25FL040A Top and Bottom
      flash chips.  Flash Center does not support non-uniform flash:
      Flash Center will incorrectly complain about expanding 
      erase sectors for top and bottom boot layouts since Flash Center 
      can only work with uniform sectors, and the eraseSize is
      set to the largest sector on each flash.
    -->
    <!--
    <device version="1.0">
        <deviceName>S25FL040A Top</deviceName>
        <deviceDescription>
            Spansion S25FL040A 512 Kilobyte SPI Flash, top boot
        </deviceDescription>
        <capacity>512*1024</capacity>
        <eraseSize>64*1024</eraseSize>
        <eraseAllTime>3000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x25</expectedDeviceId>
    </device>

    <device version="1.0">
        <deviceName>S25FL040A Bottom</deviceName>
        <deviceDescription>
            Spansion S25FL040A 512 Kilobyte SPI Flash, bottom boot
        </deviceDescription>
        <capacity>512*1024</capacity>
        <eraseSize>64*1024</eraseSize>
        <eraseAllTime>3000000</eraseAllTime>
        <expectedDeviceId>\x01\x02\x26</expectedDeviceId>
    </device>
    -->

    <device version="1.0">
        <deviceName>S25FL128P 64KB</deviceName>
        <deviceDescription>
            Spansion S25FL128P 16 Megabyte SPI Flash, 64 KB sectors
        </deviceDescription>
        <capacity>16*1024*1024</capacity>
        <maxBitrate>104*1000</maxBitrate>
        <writeSize>256</writeSize>
        <writeTime>1500</writeTime>
        <writeStatusRegisterTime>100000</writeStatusRegisterTime>
        <blockProtectBits>0x3c</blockProtectBits>
        <expectedDeviceId>\x01\x20\x18\x03\x01</expectedDeviceId>
        <eraseAllTime>128000000</eraseAllTime>
        <deviceConnectedMask>0x20</deviceConnectedMask>
    </device>

    <device version="1.0">
        <deviceName>S25FL128P 256KB</deviceName>
        <deviceDescription>
            Spansion S25FL128P 16 Megabyte SPI Flash, 256 KB sectors
        </deviceDescription>
        <capacity>16*1024*1024</capacity>
        <maxBitrate>104*1000</maxBitrate>
        <writeStatusRegisterTime>100000</writeStatusRegisterTime>
        <expectedDeviceId>\x01\x20\x18\x03\x00</expectedDeviceId>
        <eraseSize>256*1024</eraseSize>
        <eraseTime>2000000</eraseTime>
        <eraseAllTime>128000000</eraseAllTime>
        <deviceConnectedMask>0x20</deviceConnectedMask>
    </device>

</devices>

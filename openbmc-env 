#### Delete the previous machine conf settings ####### 
rm -rf build/conf

if [ -n "$BASH_SOURCE" ]; then
   OBMCROOT="`dirname $BASH_SOURCE`"
elif [ -n "$ZSH_NAME" ]; then
   OBMCROOT="`dirname $0`"
else
   OBMCROOT="`pwd`"
fi

if [ -f $OBMCROOT/customrc ]; then
    echo "### Sourcing customrc ###"
    . $OBMCROOT/customrc
fi

echo "### Initializing OE build env for X86 ###"
TEMPLATECONF=meta-phosphor/conf/ppc-without-ibmInternal source $OBMCROOT/oe-init-build-env

